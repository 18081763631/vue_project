{
	"easycom": {
		// npm安装的方式不需要前面的"@/"，下载安装的方式需要"@/"
		// npm安装方式
		// "^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
		// 下载安装方式
		"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		}
	    ,{
            "path" : "pages/user/index",
            "style" : {
				"navigationBarTitleText": "个人"
			}
        },{
            "path" : "pages/function/AnnouncementNotice",
            "style" : {
				"navigationBarTitleText": "公告通知"
			}
        },{
            "path" : "pages/function/ApplicationRequest",
            "style" : {
				"navigationBarTitleText": "盖章申请"
			}
        },{
            "path" : "pages/function/ItemPurchase",
            "style" : {
				"navigationBarTitleText": "物品申购"
			}
        },{
            "path" : "pages/function/MaintenanceRequest",
            "style" : {
				"navigationBarTitleText": "维修申请"
			}
        },{
            "path" : "pages/function/WorkOvertime",
            "style" : {
				"navigationBarTitleText": "社工加班"
			}
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
	  "color": "#515151",
	  "selectedColor": "#1296db",
	  "borderStyle": "white",
	  "backgroundColor": "#ffffff",
	  "list": [
	    {
	      "pagePath": "pages/index/index",
	      "iconPath": "static/business2.png",
	      "selectedIconPath": "static/business1.png",
	      "text": "业务"
	    },
	    {
	      "pagePath": "pages/user/index",
	      "iconPath": "static/personal2.png",
	      "selectedIconPath": "static/personal1.png",
	      "text": "个人"
	    }
	  ]
	}
}
